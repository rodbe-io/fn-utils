{
  "name": "@rodbe/fn-utils",
  "version": "1.11.0",
  "description": "RBX npm Library",
  "keywords": [
    "typescript",
    "front-end"
  ],
  "homepage": "https://github.com/rodbe-io/fn-utils",
  "bugs": {
    "url": "https://github.com/rodbe-io/fn-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodbe-io/fn-utils.git"
  },
  "license": "MIT",
  "author": "kevinRodbe",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm run type-check",
    "build": "pkgroll",
    "commitlint": "commitlint --edit",
    "prepare": "husky || true",
    "release": "semantic-release",
    "start": "pkgroll --watch",
    "test:unit": "vitest run",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/format": "^20.2.0",
    "@commitlint/types": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@rodbe/eslint-config": "^1.4.0",
    "@rodbe/prettier-config": "^1.5.0",
    "@rodbe/tsconfig": "^1.5.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-package-json": "^0.85.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "pkgroll": "^2.21.4",
    "prettier": "^3.7.4",
    "prettier-plugin-curly": "^0.4.1",
    "prettier-plugin-packagejson": "^2.5.20",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": ">= 24.10.0",
    "npm": ">= 10.9.2",
    "pnpm": ">= 10.5.2"
  },
  "volta": {
    "node": "25.2.1",
    "pnpm": "10.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "enginesStrict": true
}
